{
  "name": "graphql-explorer-html",
  "version": "1.0.0-alpha.6",
  "main": "build/main.js",
  "module": "build/es/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/graphql-explorer.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "build": "webpack --bail --mode production",
    "format": "4c format src './*'",
    "lint": "4c lint src './*'",
    "prebuild": "rimraf build",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "start": "DEV_SERVER=true webpack serve --mode development",
    "test": "yarn lint && yarn typecheck",
    "typecheck": "yarn run -T tsc --noEmit -p tsconfig.build.json"
  },
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@4c/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "yarn 4c lint --fix"
  },
  "devDependencies": {
    "@4c/cli": "^4.0.4",
    "@4c/prettier-config": "^1.1.0",
    "@4c/tsconfig": "^0.4.1",
    "@types/lodash": "^4.17.21",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint-config-4catalyzer-react": "^1.1.3",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "webpack": "^5.99.0",
    "webpack-atoms": "^17.1.0",
    "webpack-cli": "^6.0.0",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@apollo/client": "^3.12.0",
    "astroturf": "^1.2.0",
    "core-js": "^3.41.0",
    "graphql": "^16.12.0",
    "graphql-explorer": "^1.0.0-alpha.6",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.0",
    "react-dom": "^19.0.0",
    "react-formal": "^2.4.4",
    "yup": "^1.6.0"
  }
}
