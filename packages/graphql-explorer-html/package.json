  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "jsdom"
  },
  "release": {
    "pkgRoot": "lib",
    "extends": [
      "@4c/semantic-release-config"
    ]
  },
  "devDependencies": {
    "@4c/babel-preset": "^7.1.0",
    "@4c/cli": "^1.0.1",
    "@4c/import-sort": "^4.3.3",
    "@4c/jest-preset": "^1.3.0",
    "@4c/prettier-config": "^1.1.0",
    "@4c/semantic-release-config": "^2.0.3",
    "@4c/tsconfig": "^0.3.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-typescript": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "eslint": "^6.5.1",
    "eslint-config-4catalyzer-jest": "^2.0.1",
    "eslint-config-4catalyzer-react": "^1.0.0",
    "eslint-config-4catalyzer-typescript": "^1.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^3.0.9",
    "jest": "^24.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.15.3",
    "semantic-release": "^15.5.0",
    "travis-deploy-once": "^5.0.0"
  },
  "dependencies": {
    "@types/react": "^16.9.10",
    "@types/react-dom": "^16.9.2",
    "apollo-boost": "^0.4.4",
    "apollo-link-http": "^1.5.16",
    "bootstrap": "^4.3.1",
    "graphql-tools": "^4.0.6",
    "query-string": "^6.8.3",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.11.0"
  }
