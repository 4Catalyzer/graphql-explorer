    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint-config-4catalyzer-react": "^1.3.0",
    "eslint-config-4catalyzer-typescript": "^3.3.0",
