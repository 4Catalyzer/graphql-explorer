    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint-config-4catalyzer-react": "^1.1.3",
    "eslint-config-4catalyzer-typescript": "^3.1.2",
