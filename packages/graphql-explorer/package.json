    "@apollo/react-hooks": "^3.1.3",
    "@types/lodash": "^4.14.144",
    "@types/react": "^16.9.10",
    "@types/react-dom": "^16.9.2",
    "@types/react-widgets": "^4.3.3",
    "@types/yup": "^0.26.33",
    "apollo-boost": "^0.4.7",
    "apollo-link-http": "^1.5.16",
    "bootstrap": "^4.3.1",
    "copy-to-clipboard": "^3.2.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "lodash": "^4.17.15",
    "query-string": "^6.8.3",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-formal": "^1.0.1",
    "react-icons": "^3.7.0",
    "react-widgets": "^5.0.0-beta.0",
    "yup": "^0.28.0"
