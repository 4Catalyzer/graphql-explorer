{
  "name": "graphql-explorer",
  "version": "1.0.0-alpha.6",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/graphql-explorer.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "xxx:remove-relative-css": "sed -i -e 's/background: url(\"..\\/img\\/loader-big.gif\") no-repeat center;//' styles/style.css",
    "build": "4c build --copy-files false src && yarn build:styles",
    "build:styles": "sass src:styles && yarn xxx:remove-relative-css",
    "format": "4c format src './*'",
    "lint": "4c lint src './*'",
    "prepublishOnly": "yarn run build",
    "tdd": "jest --watch",
    "test": "yarn lint && yarn typecheck && yarn build",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "testonly": "jest"
  },
  "files": [
    "lib",
    "styles"
  ],
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@4c/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "yarn 4c lint --fix"
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@4c/babel-preset": "^10.2.1",
    "@4c/jest-preset": "^1.8.1",
    "@4c/prettier-config": "^1.1.0",
    "@4c/tsconfig": "^0.4.1",
    "@babel/preset-typescript": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint-config-4catalyzer-jest": "^2.3.0",
    "eslint-config-4catalyzer-react": "^1.3.0",
    "eslint-config-4catalyzer-typescript": "^3.3.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "travis-deploy-once": "^5.0.11"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@apollo/react-hooks": "^4.0.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "apollo-link-http": "^1.5.17",
    "bootstrap": "^5.2.3",
    "copy-to-clipboard": "^3.3.3",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "react-bootstrap": "^2.7.0",
    "react-formal": "^2.7.0",
    "react-icons": "^4.7.1",
    "react-widgets": "5.8.4",
    "sass": "^1.58.0",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "graphql": "^15.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
