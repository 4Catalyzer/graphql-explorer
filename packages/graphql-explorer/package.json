{
  "name": "graphql-explorer",
  "version": "0.4.0",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/graphql-explorer.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "xxx:remove-relative-css": "sed -i -e 's/background: url(\"..\\/img\\/loader-big.gif\") no-repeat center;//' lib/style.css",
    "build": "4c build --copy-files false src && yarn build:styles",
    "build:styles": "node-sass src -o lib && yarn xxx:remove-relative-css",
    "format": "4c format src './*'",
    "lint": "4c lint src './*'",
    "prepublishOnly": "yarn run build",
    "tdd": "jest --watch",
    "test": "yarn lint && yarn typecheck && yarn build",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "testonly": "jest"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@4c/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "yarn 4c lint --fix"
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@4c/babel-preset": "^7.4.1",
    "@4c/jest-preset": "^1.5.3",
    "@4c/prettier-config": "^1.1.0",
    "@4c/tsconfig": "^0.3.1",
    "@babel/preset-typescript": "^7.10.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint-config-4catalyzer-jest": "^2.0.8",
    "eslint-config-4catalyzer-react": "^1.0.10",
    "eslint-config-4catalyzer-typescript": "^2.0.4",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "travis-deploy-once": "^5.0.11"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@apollo/react-hooks": "^4.0.0",
    "@types/lodash": "^4.14.144",
    "@types/react": "^16.9.10",
    "@types/react-dom": "^16.9.2",
    "@types/yup": "^0.29.0",
    "apollo-link-http": "^1.5.16",
    "bootstrap": "^4.5.2",
    "copy-to-clipboard": "^3.2.0",
    "graphql": "^15.3.0",
    "lodash": "^4.17.20",
    "query-string": "^6.13.1",
    "react-bootstrap": "^1.3.0",
    "react-formal": "^2.1.2",
    "react-icons": "^3.11.0",
    "react-widgets": "5.0.0-beta.16",
    "yup": "^0.29.0"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
