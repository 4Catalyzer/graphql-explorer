{
  "plugins": ["prettier"],
  "extends": [
    "4catalyzer-react",
    "4catalyzer-jest",
    "prettier",
    "prettier/react",
    "4catalyzer-typescript"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "prettier/prettier": "error",
    "@typescript-eslint/explicit-module-boundary-types": 0,
    "import/no-cycle": 0
  }
}
