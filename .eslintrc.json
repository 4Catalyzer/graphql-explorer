{
  "plugins": ["prettier"],
  "extends": [
    "4catalyzer-react",
    "4catalyzer-jest",
    "prettier",
    "prettier/react",
    "4catalyzer-typescript"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "prettier/prettier": "error",
    "import/no-cycle": 0,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true,
        "optionalDependencies": false,
        "peerDependencies": true
      }
    ]
  }
}
